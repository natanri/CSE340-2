<%- messages() %>
<section class="account-management">
    <h1><%= (title) %></h1>
    <h2>Welcome, <%= accountData.account_firstname %></h2>
    <p>You're Logged in as <%= accountData.account_type %></p>
    <a href="/account/updateAccount/<%= account_id  %>">Edit account information</a>

    <% if(accountData.account_type === "Employee" ||accountData.account_type === "Admin") { %> 
        <!--Inventory management link for employee and admin-->
            <h2>Inventory Management</h2>
            <p><a href="/inv/management">Management Inventory</a></p>
    <% } %>
</section>

